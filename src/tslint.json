{
  "extends": "../tslint.json",
  "rules": {
    "indent": {
      "options": ["tabs", 4]
    },
    "no-inferrable-types": [
      "ignore-properties"
    ],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
